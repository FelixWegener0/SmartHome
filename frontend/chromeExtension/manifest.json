{
    "name": "Hygrometer Extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["scripting", "alarms", "activeTab", "notifications"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
        "default_icon": "icon.png"
    }  
}